{
    "name": "ryanwhitman/php-values",
    "description": "PHP Values",
    "license": "MIT",
    "keywords": [
        "value objects"
    ],
    "authors": [
        {
            "name": "Ryan Whitman",
            "homepage": "https://github.com/RyanWhitman"
        }
    ],
    "homepage": "https://github.com/RyanWhitman/php-values",
    "support": {
        "issues": "https://github.com/RyanWhitman/php-values/issues",
        "source": "https://github.com/RyanWhitman/php-values",
        "docs": "https://github.com/RyanWhitman/php-values/blob/main/README.md"
    },
    "require": {
        "php": ">=7.4 <8.5",
        "doctrine/annotations": "^1|^2"
    },
    "require-dev": {
        "filp/whoops": "^2.14",
        "oyova/php-cs-fixer": "^1.5",
        "pestphp/pest": "^1.22",
        "symfony/var-dumper": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "RyanWhitman\\PhpValues\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RyanWhitman\\PhpValues\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
